package arrays;

public class armstrong {
	public boolean arm(int x) {
		int sum=0;
		int org=x;
		while(x>0) {
			 int ld=x%10;
			sum=sum+(ld*ld*ld);
			x=x/10;
			
		}
		return org==sum;
	}

	public static void main(String[] args) {
		int[] arr= {1,3,4,6};
		armstrong as=new armstrong();
		System.out.print(as.arm(153));
		
		

	}

}
